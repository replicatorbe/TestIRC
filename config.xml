<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Baboon Tchatche</name>
    <description>
        Le tchatche des babouins sur baboon.fr
    </description>
    <author email="jerome@fafchamps.be" href="http://www.baboon.fr">
       Jérôme Fafchamps
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-legacy-whitelist" spec="1.1.1" />
    <plugin name="cordova-plugin-vibration" source="npm" spec="~2.1.0"/>
    <plugin name="cordova-plugin-whitelist"      source="npm" spec="1.1.0" />
<allow-navigation href="*" />
<allow-intent href="*" />
<access origin="*" />
    <platform name="android">
              <icon src="res/icon/android/ldpi.png" density="ldpi" />
              <icon src="res/icon/android/mdpi.png" density="mdpi" />
              <icon src="res/icon/android/hdpi.png" density="hdpi" />
              <icon src="res/icon/sandroid/xhdpi.png" density="xhdpi" />
     </platform>
</widget>
